local function KHTCSXA_fake_script() -- KillAura.KA 
	local script = Instance.new('LocalScript', KillAura)

	
	
	_G.KillAura = false
	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.BackgroundColor3 == Color3.fromRGB(4, 58, 0) then
			_G.KillAura = true
			if _G.KillAura == true then
				local OPname = ""
				local distanceThreshold = 15
	
				local function isPlayerNearby()
					local player = game.Players.LocalPlayer
					if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
						local playerPosition = player.Character.HumanoidRootPart.Position
	
						for _, otherPlayer in pairs(game.Players:GetPlayers()) do
							if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
								local otherPosition = otherPlayer.Character.HumanoidRootPart.Position
								local distance = (playerPosition - otherPosition).Magnitude
								if distance <= distanceThreshold then
									OPname = otherPlayer.Name
									wait(0.1)
									return true
								end
							end
						end
					end
					return false
				end
	
				local function fa()
					while _G.KillAura == true do
						isPlayerNearby()
						wait(0.1) 
						local args = {
							[1] = workspace:WaitForChild(OPname)
						}
	
						game:GetService("ReplicatedStorage"):WaitForChild("GameRemotes"):WaitForChild("Attack"):InvokeServer(unpack(args))
					end
				end
				fa()
			end
		else
			_G.KillAura = false
		end
	end)
end
coroutine.wrap(KHTCSXA_fake_script)()
local function NQKUQBM_fake_script() -- Frame.on/off 
	local script = Instance.new('LocalScript', Frame)

	local loc = script.Parent
	
	loc["Kill Aura"].MouseButton1Click:Connect(function()
		if loc["Kill Aura"].BackgroundColor3 == Color3.fromRGB(0, 24, 33) then
			loc["Kill Aura"].BackgroundColor3 = Color3.fromRGB(4, 58, 0)
		else
			loc["Kill Aura"].BackgroundColor3 = Color3.fromRGB(0, 24, 33)
		end
	end)
	loc["speedHax"].MouseButton1Click:Connect(function()
		if loc["speedHax"].BackgroundColor3 == Color3.fromRGB(0, 24, 33) then
			loc["speedHax"].BackgroundColor3 = Color3.fromRGB(4, 58, 0)
		else
			loc["speedHax"].BackgroundColor3 = Color3.fromRGB(0, 24, 33)
		end
	end)
	loc["ESP"].MouseButton1Click:Connect(function()
		if loc["ESP"].BackgroundColor3 == Color3.fromRGB(0, 24, 33) then
			loc["ESP"].BackgroundColor3 = Color3.fromRGB(4, 58, 0)
		else
			loc["ESP"].BackgroundColor3 = Color3.fromRGB(0, 24, 33)
		end
	end)
	loc["instabreak"].MouseButton1Click:Connect(function()
		if loc["instabreak"].BackgroundColor3 == Color3.fromRGB(0, 24, 33) then
			loc["instabreak"].BackgroundColor3 = Color3.fromRGB(4, 58, 0)
		else
			loc["instabreak"].BackgroundColor3 = Color3.fromRGB(0, 24, 33)
		end
	end)
	
	
	
	
	
end
coroutine.wrap(NQKUQBM_fake_script)()
local function WEBI_fake_script() -- speedHax.SH 
	local script = Instance.new('LocalScript', speedHax)

	local loc = script.Parent.Parent
	local uis = game:GetService("UserInputService")
	local abled = false
	
	loc.speedHax.MouseButton1Click:Connect(function()
		abled = not abled
	end)
	
	uis.InputBegan:Connect(function(input)
		if abled == true then
			if uis:IsKeyDown(Enum.KeyCode.LeftShift) then
				local player = game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name)
	
				if player then
					player.Humanoid.WalkSpeed = 35
				end
			end
		else
			local player = game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name)
	
			if player then
				player.Humanoid.WalkSpeed = 16
			end
		end
	
	end)
	
	uis.InputEnded:Connect(function(input)
		if abled == true then
			if input.KeyCode == Enum.KeyCode.LeftShift then
				local player = game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name)
	
				if player then
					player.Humanoid.WalkSpeed = 16
				end
			end
	
		end
	end)
	
end
coroutine.wrap(WEBI_fake_script)()
local function YNEBK_fake_script() -- ESP.esp 
	local script = Instance.new('LocalScript', ESP)

	local loc = script.Parent.Parent
	local abled = false
	
	loc.ESP.MouseButton1Click:Connect(function()
		abled = not abled
	end)
	
	for idk = 1, math.huge do
		wait(10)
			if abled == true then
			for _, player in ipairs(game.Players:GetPlayers()) do
				local character = player.Character
				if character then
					local highlight = character:FindFirstChild("Highlight")
	
					if not highlight then
						if player.Name ~= game.Players.LocalPlayer.Name then
							local newHighlight = Instance.new("Highlight")
							newHighlight.Parent = character
							newHighlight.Enabled = true
							newHighlight.Adornee = character
							newHighlight.FillColor = Color3.fromRGB(0, 40, 75)
							newHighlight.FillTransparency = 0.3
							newHighlight.OutlineTransparency = 0.2
							newHighlight.OutlineColor = Color3.fromRGB(0, 98, 255)
						end
	
					end
				end
			end
		end
	end
	
end
coroutine.wrap(YNEBK_fake_script)()
local function ADNQE_fake_script() -- instabreak.IB 
	local script = Instance.new('LocalScript', instabreak)

	local loc = script.Parent.Parent
	
	_G.be = false
	loc.instabreak.MouseButton1Click:Connect(function()
		if _G.be == false then
			_G.be = true
			local function df()
				while _G.be == true do
					game:GetService("ReplicatedStorage"):WaitForChild("GameRemotes"):WaitForChild("AcceptBreakBlock"):InvokeServer()
					task.wait()
				end
			end
			df()
		else
			_G.be = false
			local function df()
				while _G.be == true do
					game:GetService("ReplicatedStorage"):WaitForChild("GameRemotes"):WaitForChild("AcceptBreakBlock"):InvokeServer()
					task.wait()
				end
			end
			df()
		end
	
	
	end)
end
coroutine.wrap(ADNQE_fake_script)()
local function AMQTF_fake_script() -- Frame_2.on/off 
	local script = Instance.new('LocalScript', Frame_2)

	local loc = script.Parent
	
	loc["nuker"].MouseButton1Click:Connect(function()
		if loc["nuker"].BackgroundColor3 == Color3.fromRGB(0, 24, 33) then
			loc["nuker"].BackgroundColor3 = Color3.fromRGB(4, 58, 0)
		else
			loc["nuker"].BackgroundColor3 = Color3.fromRGB(0, 24, 33)
		end
	end)
	loc["excavator"].MouseButton1Click:Connect(function()
		if loc["excavator"].BackgroundColor3 == Color3.fromRGB(0, 24, 33) then
			loc["excavator"].BackgroundColor3 = Color3.fromRGB(4, 58, 0)
		else
			loc["excavator"].BackgroundColor3 = Color3.fromRGB(0, 24, 33)
		end
	end)
	
	
	
	
	
end
coroutine.wrap(AMQTF_fake_script)()
local function XZQZ_fake_script() -- nuker.bomb bomb 
	local script = Instance.new('LocalScript', nuker)

	local loc = script.Parent.Parent
	local abled = true
	
	loc.nuker.MouseButton1Click:Connect(function()
		abled = not abled
	end)
	
	
	local function boom()
		while abled do
			local word = game:GetService("Players").LocalPlayer.PlayerGui.HUDGui.DataFrame.Coord.Text
			wait()
	
			local function extract_numbers(str)
				local x, y, z = str:match("(-?%d+),%s*(-?%d+),%s*(-?%d+)")
				return tonumber(x), tonumber(y), tonumber(z)
			end
	
			-- Extract numbers and assign to variables
			local x, y, z = extract_numbers(word)
	
			for zz = 0, 4 do
				for xx = 0, 4 do
					for yy = 0, 4 do
						local args = {
							[1] = x - math.random(0, 4),
							[2] = y - math.random(0, 4),
							[3] = z - math.random(0, 4)
						}
	
						game:GetService("ReplicatedStorage"):WaitForChild("GameRemotes"):WaitForChild("BreakBlock"):FireServer(unpack(args))
						task.wait(0.02)
						game:GetService("ReplicatedStorage"):WaitForChild("GameRemotes"):WaitForChild("AcceptBreakBlock"):InvokeServer()
					end
				end
			end
		end
	end
	boom()
	
	
end
coroutine.wrap(XZQZ_fake_script)()
local function KDRSWK_fake_script() -- excavator.mine 
	local script = Instance.new('LocalScript', excavator)

	local loc = script.Parent.Parent
	local abled = true
	
	loc.excavator.MouseButton1Click:Connect(function()
		abled = not abled
	end)
	
	
	local function boom()
		while abled do
			local word = game:GetService("Players").LocalPlayer.PlayerGui.HUDGui.DataFrame.Coord.Text
			wait()
	
			local function extract_numbers(str)
				local x, y, z = str:match("(-?%d+),%s*(-?%d+),%s*(-?%d+)")
				return tonumber(x), tonumber(y), tonumber(z)
			end
	
			-- Extract numbers and assign to variables
			local x, y, z = extract_numbers(word)
	
			for zz = 0, 4 do
				for xx = 0, 4 do
					for yy = 0, 4 do
						local args = {
							[1] = x - xx,
							[2] = y - yy,
							[3] = z - zz
						}
	
						game:GetService("ReplicatedStorage"):WaitForChild("GameRemotes"):WaitForChild("BreakBlock"):FireServer(unpack(args))
						task.wait(0.02)
						game:GetService("ReplicatedStorage"):WaitForChild("GameRemotes"):WaitForChild("AcceptBreakBlock"):InvokeServer()
					end
				end
			end
		end
	end
	boom()
	
	
end
coroutine.wrap(KDRSWK_fake_script)()
