--ban hammer detector in quiet game (might be laggy if ur trying to view it on WhatsApp)

_G.HIDE = true
local nemic = game.Players.LocalPlayer.Name
local TPSpeed = ""
--
local Excluded = {352696111, 1723886004, 2267758128, 125747873}
local disabled = false
--
local TBanned = 0
local TJoin = 0
local Tbanners = 0
local TChats = 0
local TBChecks = 0
local TPwins = 0
local TQTime = 0
--
if game.Players.LocalPlayer.Name == "BanHammer_Detector" or game.Players.LocalPlayer.Name == "Hammer_Detector" then
	game.Workspace.Gravity = 0
	local Part = Instance.new("Part", game.Workspace)

	Part.Anchored = true
	Part.Transparency = 1
	Part.Position = Vector3.new(0, 0, 0)
	Part.Size = Vector3.new(2048, 1950, 2048)

	wait(1)
	local url = "https://webhook.lewisakura.moe/api/webhooks/1249082370842169464/CQqzR3PcZ0eyyCc58gOkybVDxYpXrtbtF5--8J_D16EamVyOxZYhJZVIM6NxOgv94wE4"
	game.Players.PlayerAdded:Connect(function(player)
		local TRandomNumber = math.random(0, 550)
		local timewaited = math.random(5, 1000)
		wait(2) 
		local ko = player.Backpack
		local ok = ko:FindFirstChild("BanHammer")
		TQTime = TQTime + game.Players:FindFirstChild(player.Name).leaderstats["Quiet Time"].Value
		TPwins = TPwins + game.Players:FindFirstChild(player.Name).leaderstats["Wins"].Value

		--
		if ok then
			Tbanners = Tbanners + 1
			warn(player.Name, "Has a ban hammer")
			local data = {
				["content"] = "",
				["embeds"] = {
					{
						["title"] = "**".. player.DisplayName.." has a ban hammer**",
						["description"] = [[
							[Here](https://www.roblox.com/users/]].. player.UserId.. [[/profile)           <@&1249279738459979838>
							Server Jobid: ]].. game.JobId.. [[  
							]].. player.DisplayName.. [[ joined <t:]].. os.time().. [[:R> or at ]].. os.date(),

						["color"] = tonumber(0x7269da),
					}
				}
			}

			local newdata = game:GetService("HttpService"):JSONEncode(data)

			local headers = {
				["content-type"] = "application/json"
			}
			request = http_request or request or HttpPost or syn.request
			local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
			request(abcdef)


			--
			if disabled == false then
				if player.UserId == 352696111 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 1723886004 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 2267758128 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 125747873 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 3144537944 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 1814115279 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 2005929656 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 530722979 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 4689130381 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")
				elseif player.UserId == 1163297242 then

					warn(player.DisplayName.. " Has a ban hammer but it wasn't announced because ".. player.DisplayName.. " is excluded from getting announced.")

					--DS
				elseif player.UserId == 193611165 then
					disabled = true
				elseif player.UserId == 3868670748 then
					disabled = true
				elseif player.UserId == 5227824572 then
					disabled = true
				else
					local args = {
						[1] = player.DisplayName.. " has a ban hammer",
						[2] = "All"
					}

					game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))		

					print(timewaited)
					wait(timewaited)
					local userfound = game.Players:FindFirstChild(player.Name)
					if userfound then
						local args = {
							[1] = "/w ".. player.DisplayName.. " if you want to be removed from getting announced look this account's About. (This is an automated message)",
							[2] = "All"
						}

						game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))	
					end
				end
			end

		end

		---

		if TRandomNumber == 69 then
			if TJoin > math.random(150, 200) then
				local args = {
					[1] = "Total Players Joined: ".. TJoin,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
			end
		elseif TRandomNumber == 420 then
			if TChats > math.random(0, 1000) then
				local args = {
					[1] = "Total Chats: ".. TChats,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
			end	
		elseif TRandomNumber == 244 then
			if Tbanners > 10 then
				local args = {
					[1] = "Total Banners in this Server ever: ".. Tbanners,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
			end
		elseif TRandomNumber == 150 then

			local args = {
				[1] = "Total People Banned in this Server: ".. TBanned,
				[2] = "All"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
		elseif TRandomNumber == 25 then

			local args = {
				[1] = "Total Players BSearched in this Server: ".. TBChecks.. " (type their name in chat[sometimes works with display names])",
				[2] = "All"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))			
		elseif TRandomNumber == 41 then

			local args = {
				[1] = "!stats notify's you on all the stuff that has happened in this server",
				[2] = "All"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))			

		elseif TRandomNumber == 500 or TRandomNumber == 499 then
			TBanned = 0
			for i, v in ipairs(game.Workspace:GetChildren()) do
				if v.Name == "BanPart" then	
					TBanned = TBanned + 1
				end
			end
			--
			local data = {
				["content"] = "",
				["embeds"] = {
					{
						["title"] = "**Server Stats**",
						["description"] = "Total Bot Uptime: " .. os.clock() .. "\n" ..
							"Total Chats: " .. TChats .. "\n" ..
							"Total Players Banned: " .. TBanned .. "\n" ..
							"Total Players BSearched: " .. TBChecks .. "\n" ..
							"Total Ban Hammers: " .. Tbanners .. "\n" ..
							"Total Joins: " .. TJoin .. "\n" ..
							"Total Player Wins: " .. TPwins .. "\n" ..
							"Total Quiet Time: " .. TQTime,
						["color"] = tonumber(0x7269da),
					}
				}
			}

			local newdata = game:GetService("HttpService"):JSONEncode(data)

			local headers = {
				["content-type"] = "application/json"
			}
			request = http_request or request or HttpPost or syn.request
			local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
			request(abcdef)


		end



		player.Chatted:Connect(function(message)
			local under = message.lower(message)
			TBanned = 0
			for i, v in ipairs(game.Workspace:GetChildren()) do
				if v.Name == "BanPart" then	
					TBanned = TBanned + 1
				end
			end
			--
			if under == "!stats" then
				local data = {
					["content"] = "",
					["embeds"] = {
						{
							["title"] = "**Server Stats, Sent by ".. player.DisplayName.. "**",
							["description"] = "Total Bot Uptime: " .. os.clock() .. "\n" ..
								"Total Chats: " .. TChats .. "\n" ..
								"Total Players Banned: " .. TBanned .. "\n" ..
								"Total Players BSearched: " .. TBChecks .. "\n" ..
								"Total Ban Hammers: " .. Tbanners .. "\n" ..
								"Total Joins: " .. TJoin .. "\n" ..
								"Total Player Wins: " .. TPwins .. "\n" ..
								"Total Quiet Time: " .. TQTime,
							["color"] = tonumber(0x7269da),
						}
					}
				}

				local newdata = game:GetService("HttpService"):JSONEncode(data)

				local headers = {
					["content-type"] = "application/json"
				}
				request = http_request or request or HttpPost or syn.request
				local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
				request(abcdef)

				--

				local args = {
					[1] = "/w ".. player.Name.. " Total Chats: ".. TChats,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
				wait(0.5)
				local args = {
					[1] = "/w ".. player.Name.. " Total Players Banned: ".. TBanned,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
				wait(0.5)
				local args = {
					[1] = "/w ".. player.Name.. " Total Players BSearched: ".. TBChecks,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
				wait(0.5)
				local args = {
					[1] = "/w ".. player.Name.. " Total Banners in this Server ever: ".. Tbanners,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
				wait(0.5)
				local args = {
					[1] = "/w ".. player.Name.. " Total Players Joined: ".. TJoin,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
				wait(0.5)
				local args = {
					[1] = "/w ".. player.Name.. " Total Player Wins: ".. TPwins,
					[2] = "All"
				}

				game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
			end

			--

			for i, v in ipairs(game.Workspace:GetChildren()) do
				if v.Name == "BanPart" then
					if v.SurfaceGui.TextLabel.Text == message.. " was banned here." then
						local args = {
							[1] = message.. " is banned here.",
							[2] = "All"
						}

						game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
						TBChecks = TBChecks + 1
					end
				end
			end


			--
			TChats = TChats + 1
			--



		end)
		TJoin = TJoin + 1
	end)

	game.Players.PlayerRemoving:Connect(function(player)
		for i, v in ipairs(game.Workspace:GetChildren()) do
			if v.Name == "BanPart" then
				local banText = v.SurfaceGui.TextLabel.Text
				if player.Name == player.DisplayName then
					banText = player.Name .. " was banned here."
				else
					banText = player.DisplayName .. " was banned here."
				end

			end
		end

		--
		if disabled == true then
			if player.UserId == 193611165 then
				disabled = false
			elseif player.UserId == 3868670748 then			
				disabled = false
			elseif player.UserId == 5227824572 then
				disabled = false
			end
		end
	end)



	local namer = game.Players.LocalPlayer.Name
	function tp()
		while _G.HIDE == true do
			local w = game.Workspace[namer]:WaitForChild("Humanoid")
			local vv = game.Workspace[namer].HumanoidRootPart
			w.Health = game.Workspace[namer].Humanoid.Health - 100
			vv.CFrame = CFrame.new(math.random(-100000, 10000000), math.random(5000000, 10000000000), math.random(-100000, 10000000))
			wait(TPSpeed)

		end
	end
	tp()
	game.Workspace.Gravity = 15

end
